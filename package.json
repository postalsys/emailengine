{
    "name": "emailengine-app",
    "version": "2.31.10",
    "private": true,
    "productTitle": "EmailEngine",
    "description": "Email Sync Engine",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "EENGINE_LOG_RAW=true node server --dbs.redis='redis://127.0.0.1:6379/9' --api.port=7003 --api.host=0.0.0.0 | tee $HOME/dev.txt  | pino-pretty",
        "single": "EENGINE_LOG_RAW=true node server --dbs.redis='redis://127.0.0.1:6379/10' --api.port=7003 --api.host=0.0.0.0 | tee $HOME/log.txt | pino-pretty",
        "raw": "NODE_OPTIONS=--no-experimental-fetch EENGINE_LOG_RAW=true node server --dbs.redis='redis://127.0.0.1:6379/9' | eerawlog",
        "test": "grunt && node --test test/",
        "swagger": "./getswagger.sh",
        "build-source": "rm -rf node_modules package-lock.json && npm install && npm run licenses && rm -rf node_modules package-lock.json && npm install --omit=dev && rm -rf package-lock.json && ./update-info.sh",
        "build-dist": "npx pkg --compress Brotli package.json && rm -rf package-lock.json && npm install",
        "build-dist-fast": "npx pkg --debug package.json && rm -rf package-lock.json && npm install",
        "licenses": "license-checker --json | node license-table.js > static/licenses.html",
        "postinstall": "npm run licenses",
        "gettext": "find ./views -name \"*.hbs\" -print0 | xargs -0 xgettext-template -L Handlebars -o translations/messages.pot --force-po && jsxgettext lib/routes-ui.js workers/api.js -j -o translations/messages.pot"
    },
    "keywords": [
        "IMAP"
    ],
    "author": "Postal Systems OU",
    "license": "LICENSE_EMAILENGINE",
    "bin": {
        "emailengine": "bin/emailengine.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/postalsys/emailengine.git"
    },
    "bugs": {
        "url": "https://github.com/postalsys/emailengine/issues"
    },
    "homepage": "https://emailengine.app/",
    "dependencies": {
        "@bugsnag/js": "7.20.0",
        "@elastic/elasticsearch": "8.6.0",
        "@hapi/boom": "10.0.1",
        "@hapi/cookie": "12.0.1",
        "@hapi/crumb": "9.0.1",
        "@hapi/hapi": "21.3.0",
        "@hapi/inert": "7.1.0",
        "@hapi/vision": "7.0.1",
        "@phc/pbkdf2": "1.1.14",
        "@postalsys/certs": "1.0.3",
        "@postalsys/email-text-tools": "1.2.6",
        "@postalsys/hecks": "3.0.0-fork.3",
        "@postalsys/templates": "1.0.3",
        "ace-builds": "1.16.0",
        "base32.js": "0.1.0",
        "bull-arena": "3.30.4",
        "bullmq": "3.10.2",
        "compare-versions": "5.0.3",
        "dotenv": "16.0.3",
        "encoding-japanese": "2.0.0",
        "exponential-backoff": "3.1.1",
        "express": "4.18.2",
        "gettext-parser": "6.0.0",
        "handlebars": "4.7.7",
        "hapi-auth-bearer-token": "8.0.0",
        "hapi-pino": "12.0.0",
        "hapi-swagger": "16.0.1",
        "he": "1.2.0",
        "html-to-text": "9.0.5",
        "humanize": "0.0.9",
        "ical.js": "1.5.0",
        "iconv-lite": "0.6.3",
        "imapflow": "1.0.123",
        "ioredfour": "1.2.0-ioredis-07",
        "ioredis": "5.3.1",
        "ipaddr.js": "2.0.1",
        "joi": "17.9.1",
        "jquery": "3.6.4",
        "js-beautify": "1.14.7",
        "libbase64": "1.2.1",
        "libmime": "5.2.1",
        "libqp": "2.0.1",
        "license-checker": "25.0.1",
        "mailparser": "3.6.4",
        "mailsplit": "5.4.0",
        "minimist": "1.2.8",
        "msgpack5": "6.0.2",
        "murmurhash": "2.0.1",
        "nanoid": "3.3.4",
        "node-fetch": "2.6.7",
        "node-gettext": "3.0.0",
        "node-vault": "0.9.22",
        "nodemailer": "6.9.1",
        "pino": "8.11.0",
        "prom-client": "14.2.0",
        "psl": "1.9.0",
        "pubface": "1.0.4",
        "punycode": "2.3.0",
        "qrcode": "1.5.1",
        "smtp-server": "3.11.0",
        "socks": "2.7.1",
        "speakeasy": "2.0.0",
        "startbootstrap-sb-admin-2": "4.1.4",
        "timezones-list": "3.0.2",
        "uuid": "9.0.0",
        "wild-config": "1.7.0",
        "xml2js": "0.4.23"
    },
    "devDependencies": {
        "eerawlog": "1.5.0",
        "eslint": "8.36.0",
        "eslint-config-nodemailer": "1.2.0",
        "eslint-config-prettier": "8.8.0",
        "grunt": "1.6.1",
        "grunt-cli": "1.4.3",
        "grunt-eslint": "24.0.1",
        "jsxgettext": "0.11.0",
        "pino-pretty": "10.0.0",
        "pkg": "5.8.1",
        "xgettext-template": "4.1.2"
    },
    "engines": {
        "node": ">=16.x"
    },
    "pkg": {
        "scripts": [
            "workers/**/*.js"
        ],
        "assets": [
            "static/**/*",
            "views/**/*",
            "lib/lua/**/*",
            "node_modules/bull/lib/commands/**/*",
            "node_modules/swagger-ui-dist/**/*",
            "translations/**/*",
            "LICENSE_EMAILENGINE.txt",
            "help.txt",
            "version-info.json"
        ],
        "targets": [
            "node18-linux-x64",
            "node18-macos-x64",
            "node18-win-x64"
        ],
        "outputPath": "ee-dist"
    }
}
